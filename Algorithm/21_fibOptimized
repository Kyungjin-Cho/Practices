def fib_optimized(n):
    # 변수 정의
    current = 1
    previous = 0
    
    # 변수 반복문
    for i in range(1, n):
        current, previous = current + previous, current
    
    # n번째 current(피보나치 수) 리턴
    return current

# 테스트
print(fib_optimized(16))
print(fib_optimized(53))
print(fib_optimized(213))
